{
    "compileOnSave": true,
    "compilerOptions": {
      "target": "es5",
      "lib": [ "dom", "dom.iterable", "ES2015" ],
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "forceConsistentCasingInFileNames": false,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist/",
      "noImplicitAny": false,
      "noEmit": false,
      "sourceMap": true,
      "jsx": "react"
    }
  }
  